def sum_element_list(l1,l2):
    elem1 = ''
    elem2 = ''
    for i in l1:
        elem1 += str(i)
    for j in l2:
        elem2 += str(j)
        
    sum_elm = int(elem1) + int(elem2)
    
    res = [int(x) for x in str(sum_elm)]
    return res[::-1]

#l1 = [2,4,3]
#l2 = [5,6,4]

l1 = [9,9,9,9,9,9,9]; l2 = [9,9,9,9]

print(sum_element_list(l1,l2))
